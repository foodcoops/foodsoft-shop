{
  "name": "foodsoft-shop",
  "version": "0.1.0",
  "description": "Shopping cart component for Foodsoft: web-based software to manage a non-profit food coop.",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server -w -d",
    "build": "NODE_ENV=production webpack -p && mkdir -p build/css && cp node_modules/bootstrap/dist/css/bootstrap.min.css node_modules/famfamfam-flags/dist/sprite/famfamfam-flags.min.css node_modules/famfamfam-flags/dist/sprite/famfamfam-flags.png build/css/ && cp -R node_modules/bootstrap/dist/fonts build/ && sed 's/node_modules[^\"]*\\/\\([^\"]*\\)\"/css\\/\\1\"/' index.html > build/index.html && [ -e local.js ] && cp local.js build/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "wvengen <dev-voko@willem.engen.nl>",
  "license": "GPL-3.0+",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "i18n-js": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "oauth2-client-js": "0.0.15",
    "prop-types": "^15.5.10",
    "qs": "^6.1.0",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-bootstrap": "^0.31.0",
    "react-debounce-input": "^3.0.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-bootstrap": "^0.24.2",
    "react-router-dom": "^4.1.1",
    "redux": "^3.5.2",
